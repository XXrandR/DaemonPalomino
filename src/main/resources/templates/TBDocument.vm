<?xml version="1.0" encoding="utf-8" standalone="no"?>
<Invoice xmlns="urn:oasis:names:specification:ubl:schema:xsd:Invoice-2" xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2" xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2" xmlns:ds="http://www.w3.org/2000/09/xmldsig#" xmlns:ext="urn:oasis:names:specification:ubl:schema:xsd:CommonExtensionComponents-2">

  <ext:UBLExtensions>
    <ext:UBLExtension>
      <ext:ExtensionContent/>
    </ext:UBLExtension>
  </ext:UBLExtensions>

  <cbc:UBLVersionID>2.1</cbc:UBLVersionID>
  <cbc:CustomizationID>2.0</cbc:CustomizationID>
  <cbc:ID>$document.NuDocu</cbc:ID>
  <cbc:IssueDate>$document.DateIssue</cbc:IssueDate>
  <cbc:IssueTime>$document.TimeIssue</cbc:IssueTime>
  <cbc:DueDate>$document.DueDate</cbc:DueDate>
  <cbc:InvoiceTypeCode listID="0101">03</cbc:InvoiceTypeCode>
  <cbc:Note languageLocaleID="1000"><![CDATA[Cuarenta y cinco  con 00/100 ]]></cbc:Note>
  <cbc:DocumentCurrencyCode>$document.CurrencyTypeId</cbc:DocumentCurrencyCode>
  <cac:Signature>
    <cbc:ID>signaturePALOMINO/cbc:ID>
    <cbc:Note>PALOMINO</cbc:Note>
    <cac:SignatoryParty>
      <cac:PartyIdentification>
        <cbc:ID>$document.CompanyID</cbc:ID>
      </cac:PartyIdentification>
      <cac:PartyName>
        <cbc:Name><![CDATA[$document.CompanyName]]></cbc:Name>
      </cac:PartyName>
    </cac:SignatoryParty>
    <cac:DigitalSignatureAttachment>
      <cac:ExternalReference>
        <cbc:URI>#signaturePALOMINO</cbc:URI>
      </cac:ExternalReference>
    </cac:DigitalSignatureAttachment>
  </cac:Signature>
  <cac:AccountingSupplierParty>
    <cac:Party>
      <cac:PartyIdentification>
        <cbc:ID schemeID="6">$document.CompanyID</cbc:ID>
      </cac:PartyIdentification>
      <cac:PartyName>
        <cbc:Name><![CDATA[$document.CompanyName]]></cbc:Name>
      </cac:PartyName>
      <cac:PartyLegalEntity>
        <cbc:RegistrationName><![CDATA[$document.CompanyName]]></cbc:RegistrationName>
        <cac:RegistrationAddress>
          <cbc:ID>$document.DistrictId</cbc:ID>
          <cbc:AddressTypeCode>$document.CodigoAnexoSunat</cbc:AddressTypeCode>
          <cbc:CityName>$document.CityName</cbc:CityName>
          <cbc:CountrySubentity>$document.DepartmentDescription</cbc:CountrySubentity>
          <cbc:District>$document.DistrictDescription</cbc:District>
          <cac:AddressLine>
            <cbc:Line><![CDATA[$document.EstablishmentAddress]]></cbc:Line>
          </cac:AddressLine>
          <cac:Country>
            <cbc:IdentificationCode>$document.CountryId</cbc:IdentificationCode>
          </cac:Country>
        </cac:RegistrationAddress>
      </cac:PartyLegalEntity>
      <cac:Contact>
        <cbc:Telephone>.</cbc:Telephone>
        <cbc:ElectronicMail>$document.EstablishmentEmail</cbc:ElectronicMail>
      </cac:Contact>
    </cac:Party>
  </cac:AccountingSupplierParty>
  <cac:AccountingCustomerParty>
    <cac:Party>
      <cac:PartyIdentification>
        <cbc:ID schemeID="1">$document.CustomerId</cbc:ID>
      </cac:PartyIdentification>
      <cac:PartyLegalEntity>
        <cbc:RegistrationName><![CDATA[$document.CustomerName]]></cbc:RegistrationName>
      </cac:PartyLegalEntity>
    </cac:Party>
  </cac:AccountingCustomerParty>

#if ($document.FormaPago == "Credito")
  <cac:PaymentTerms>
    <cbc:ID>FormaPago</cbc:ID>
    <cbc:PaymentMeansID>$document.FormaPago</cbc:PaymentMeansID>
    <cbc:Amount currencyID="$document.CurrencyTypeId">$document.PayableAmount</cbc:Amount>
  </cac:PaymentTerms>
#else
  <cac:PaymentTerms>
    <cbc:ID>FormaPago</cbc:ID>
    <cbc:PaymentMeansID>$document.FormaPago</cbc:PaymentMeansID>
  </cac:PaymentTerms>
#end

  <cac:TaxTotal>
    <cbc:TaxAmount currencyID="$document.CurrencyTypeId">$document.TotalTaxes</cbc:TaxAmount>
    <cac:TaxSubtotal>
      <cbc:TaxableAmount currencyID="$document.CurrencyTypeId">$document.PayableAmount</cbc:TaxableAmount>
      <cbc:TaxAmount currencyID="$document.CurrencyTypeId">0</cbc:TaxAmount>
      <cac:TaxCategory>
        <cac:TaxScheme>
          <cbc:ID>9997</cbc:ID>
          <cbc:Name>EXO</cbc:Name>
          <cbc:TaxTypeCode>VAT</cbc:TaxTypeCode>
        </cac:TaxScheme>
      </cac:TaxCategory>
    </cac:TaxSubtotal>
  </cac:TaxTotal>

  <cac:LegalMonetaryTotal>
    <cbc:LineExtensionAmount currencyID="$document.CurrencyTypeId">$document.PayableAmount</cbc:LineExtensionAmount>
    <cbc:TaxInclusiveAmount currencyID="$document.CurrencyTypeId">$document.PayableAmount</cbc:TaxInclusiveAmount>
    <cbc:PayableAmount currencyID="$document.CurrencyTypeId">$document.PayableAmount</cbc:PayableAmount>
  </cac:LegalMonetaryTotal>
  <cac:InvoiceLine>

    <cbc:ID>1</cbc:ID>
    <cbc:InvoicedQuantity unitCode="NIU">1.0000</cbc:InvoicedQuantity>
    <cbc:LineExtensionAmount currencyID="$document.CurrencyTypeId">$document.PayableAmount</cbc:LineExtensionAmount>

    <cac:PricingReference>
      <cac:AlternativeConditionPrice>
        <cbc:PriceAmount currencyID="$document.CurrencyTypeId">$document.PayableAmount</cbc:PriceAmount>
        <cbc:PriceTypeCode>01</cbc:PriceTypeCode>
      </cac:AlternativeConditionPrice>
    </cac:PricingReference>

    <cac:TaxTotal>
      <cbc:TaxAmount currencyID="$document.CurrencyTypeId">$document.TotalTaxes</cbc:TaxAmount>
      <cac:TaxSubtotal>
        <cbc:TaxableAmount currencyID="$document.CurrencyTypeId">$document.ItemPrice</cbc:TaxableAmount>
        <cbc:TaxAmount currencyID="$document.CurrencyTypeId">$document.TotalTaxes</cbc:TaxAmount>
        <cac:TaxCategory>
          <cbc:Percent>$document.PercentageIgv</cbc:Percent>
          <cbc:TaxExemptionReasonCode>TaxException</cbc:TaxExemptionReasonCode>
          <cac:TaxScheme>
            <cbc:ID>9997</cbc:ID>
            <cbc:Name>EXO</cbc:Name>
            <cbc:TaxTypeCode>VAT</cbc:TaxTypeCode>
          </cac:TaxScheme>
        </cac:TaxCategory>
      </cac:TaxSubtotal>
    </cac:TaxTotal>

    <cac:Item>
      <cbc:Description><![CDATA[$document.NameProduct]]></cbc:Description>
      <cac:SellersItemIdentification>
        <cbc:ID>$document.InternalId</cbc:ID>
      </cac:SellersItemIdentification>
    </cac:Item>

    <cac:Price>
      <cbc:PriceAmount currencyID="$document.CurrencyTypeId">$document.ItemPrice</cbc:PriceAmount>
    </cac:Price>

  </cac:InvoiceLine>
</Invoice>
